<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lifemanagementapp.lifemanagementapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="singleTop"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity
            android:name=".Homescreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--style/Theme.AppCompat.Light.NoActionBar-->
        <activity
            android:name=".test_activity"
            android:label="@string/title_activity_test_activity" >
        </activity>
        <activity
            android:name=".Health"
            android:label="@string/title_activity_health" >
        </activity>
        <activity
            android:name=".Health_Form"
            android:label="@string/title_activity_health__form"
            android:parentActivityName=".Health" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.lifemanagementapp.lifemanagementapp.Health" />
        </activity>
        <activity
            android:name=".Budget"
            android:label="@string/title_activity_budget" >
        </activity>
        <activity
            android:name=".Budget_Output"
            android:label="@string/title_activity_budget_output" >
        </activity>
        <activity
            android:name=".Schedule_form"
            android:label="@string/title_activity_schedule_form" >
        </activity>
        <activity
            android:name=".GoalActivity"
            android:label="@string/title_activity_schedule_form" >
        </activity>
        <activity
            android:name=".newGoal"
            android:label="@string/title_activity_schedule_form" >
        </activity>

    </application>

</manifest>
